  {
    "name": "@axis/registry",
    "version": "0.0.1",
    "description": "AXIS MetadataLite Registry - Single Source of Truth for schema definitions",
    "type": "module",
    "main": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
      ".": "./src/index.ts",
      "./schemas": "./src/schemas/index.ts",
      "./codegen": "./src/codegen/index.ts",
      "./types": "./src/types/index.ts"
    },
    "scripts": {
      "typecheck": "tsc --noEmit",
      "codegen": "tsx src/codegen/generate.ts",
      "validate": "tsx src/codegen/validate.ts"
    },
    "dependencies": {
      "zod": "^4.3.5"
    },
    "devDependencies": {
      "tsx": "catalog:",
      "typescript": "catalog:"
    }
  }
